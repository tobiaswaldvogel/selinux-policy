(block removable

       ;;
       ;; Contexts
       ;;

       (filecon
        "/dev/mmcblk[0-9]+"
        block
        stordev_file_context)
       (filecon
        "/dev/mmcblk[0-9]+boot[0-9]+"
        block
        stordev_file_context)
       (filecon
        "/dev/mmcblk[0-9]+p[0-9]+"
        block
        stordev_file_context)
       (filecon
        "/dev/mmcblk[0-9]+rpmb"
        char
        stordev_file_context)
       (filecon
        "/dev/sr[0-9]+"
        block
        stordev_file_context)

