                  (typeattributeset
                   not_subj_typeattr
                   (not
                    subj_typeattr))

                  (neverallow not_subj_typeattr obj_typeattr
                              (file (append write)))

                  (call subj_type (unconfined.subj_typeattr))))

    (block exceptsec

           ;;
           ;; Policy
