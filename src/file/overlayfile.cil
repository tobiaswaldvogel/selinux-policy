(in .file

    ;;
    ;; Contexts
    ;;

    (filecon
     "/overlay"
     dir
     overlayfile_file_context)
    (filecon
     "/overlay/.*"
     any
     overlayfile_file_context)

    (filecon
     "/tmp/root/work"
     dir
     overlayfile_file_context)
    (filecon
     "/tmp/root/work/.*"
     any
     overlayfile_file_context)

    ;;
    ;; Macros
    ;;

    (macro obj_type_transition_overlayfile ((type ARG1))
           (call .sys.rootfile_obj_type_transition
                 (ARG1 overlayfile dir "overlay")))

    (macro tmp_obj_type_transition_overlayfile ((type ARG1))
           (call .tmp.fs_obj_type_transition
                 (ARG1 overlayfile dir "work")))

    ;;
    ;; Policy
    ;;

    (blockinherit overlay.obj_template)

    (call obj_type_transition_overlayfile (unconfined.subj_typeattr))
    (call tmp_obj_type_transition_overlayfile (unconfined.subj_typeattr))

    (call .sys.mountpoint.obj_type (overlayfile))

    (call .tmp.associate_filesystems (overlayfile))
    (call .xattr.associate_filesystems (overlayfile))

    (block overlay

           ;;
           ;; Policy
           ;;

           (blockinherit file.obj_all_macro_template)

           (call file.obj_type (obj_typeattr))

           (call .xattr.associate_filesystems (obj_typeattr))

           ;;
           ;; Templates
           ;;

           (block obj_base_template

                  ;;
                  ;; Contexts
                  ;;

                  (context
                   overlayfile_file_context
                   (.u
                    .r
                    overlayfile
                    (systemlow
                     systemlow)))

                  ;;
                  ;; Policy
                  ;;

                  (blockabstract obj_base_template)

                  (type
                   overlayfile)

                  (call .file.overlay.obj_type (overlayfile)))

