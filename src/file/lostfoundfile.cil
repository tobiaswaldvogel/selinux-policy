(in .file

    ;;
    ;; Contexts
    ;;

    (filecon
     "/lost\+found"
     dir
     lostfoundfile_file_context)
    (filecon
     "/lost\+found/.*"
     any
     ())

    (filecon
     "/mnt(/.*)?/lost\+found"
     dir
     lostfoundfile_file_context)
    (filecon
     "/mnt(/.*)?/lost\+found/.*"
     any
     ())

    (filecon
     "/overlay/lost\+found"
     dir
     lostfoundfile_file_context)
    (filecon
     "/overlay/lost\+found/.*"
     any
     ())

    ;;
    ;; Macros
    ;;

    (macro mntfile_obj_type_transition_lostfoundfile ((type ARG1))
           (call .file.mntfile_obj_type_transition
                 (ARG1 lostfoundfile dir "lost+found")))

    (macro obj_type_transition_lostfoundfile ((type ARG1))
           (call .sys.rootfile_obj_type_transition
                 (ARG1 lostfoundfile dir "lost+found")))

    (macro overlayfile_obj_type_transition_lostfoundfile ((type ARG1))
           (call .file.overlayfile_obj_type_transition
                 (ARG1 lostfoundfile dir "lost+found")))

    ;;
    ;; Policy
    ;;

    (blockinherit lostfound.obj_template)

    (call mntfile_obj_type_transition_lostfoundfile (unconfined.subj_typeattr))
    (call obj_type_transition_lostfoundfile (unconfined.subj_typeattr))
    (call overlayfile_obj_type_transition_lostfoundfile
          (unconfined.subj_typeattr))

    (block lostfound

           ;;
           ;; Policy
           ;;

           (blockinherit file.obj_all_macro_template)

           (call file.obj_type (obj_typeattr))

           (call .xattr.associate_filesystems (obj_typeattr))

           ;;
           ;; Templates
           ;;

           (block obj_base_template

                  ;;
                  ;; Contexts
                  ;;

                  (context
                   lostfoundfile_file_context
                   (.u
                    .r
                    lostfoundfile
                    (systemlow
                     systemlow)))

                  ;;
                  ;; Policy
                  ;;

                  (blockabstract obj_base_template)

                  (type
                   lostfoundfile)

                  (call .file.lostfound.obj_type (lostfoundfile)))

