           (block obj_base_template

                  ;;
                  ;; Contexts
                  ;;

                  (context
                   initscriptfile_file_context
                   (.u
                    .r
                    initscriptfile
                    (systemlow
                     systemlow)))

                  ;;
                  ;; Policy
                  ;;

                  (blockabstract obj_base_template)

                  (type
                   initscriptfile)

                  (call .file.initscript.obj_type (initscriptfile)))

