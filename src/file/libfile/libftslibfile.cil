(in .file
    (call .libfts.obj_type_transition_libfile (unconfined.subj_typeattr)))

(block libfts

       ;;
       ;; Contexts
       ;;

       (filecon
        "/usr/lib/libfts\.so\..*"
        file
        libfile_file_context)

       ;;
       ;; Macros
       ;;

       (macro obj_type_transition_libfile ((type ARG1))
              (call .file.libfile_obj_type_transition
                    (ARG1 libfile file "libfts.so.0.0.0")))

       ;;
       ;; Policy
       ;;

       (blockinherit file.lib.obj_template))
