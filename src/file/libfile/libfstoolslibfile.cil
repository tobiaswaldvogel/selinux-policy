(in .file
    (call .libfstools.obj_type_transition_libfile (unconfined.subj_typeattr)))

(block libfstools

       ;;
       ;; Contexts
       ;;

       (filecon
        "/usr/lib/libfstools\.so"
        file
        libfile_file_context)

       ;;
       ;; Macros
       ;;

       (macro obj_type_transition_libfile ((type ARG1))
              (call .file.libfile_obj_type_transition
                    (ARG1 libfile file "libfstools.so")))

       ;;
       ;; Policy
       ;;

       (blockinherit file.lib.obj_template))
