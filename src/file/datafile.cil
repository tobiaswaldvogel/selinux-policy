                   datafile_file_context
                   (.u
                    .r
                    datafile
                    (systemlow
                     systemlow)))

                  ;;
                  ;; Policy
                  ;;

                  (blockabstract obj_base_template)

                  (type
                   datafile)

                  (call .file.data.obj_type (datafile)))

